#include <iostream>



int main()
{
	int n, r, e, c, outcome;
	std::cin >> n;
	for (int i = 0; i < n; i++) {
		std::cin >> r >> e >> c;
		outcome = e - c;

		if (outcome < r) {
			std::cout << "do not advertise" << std::endl;

		}
		if (outcome > r) {
			std::cout << "advertise" << std::endl;

		}
		if(outcome==r) {
			std::cout << "does not matter" << std::endl;
		}
	}
    return 0;
}
